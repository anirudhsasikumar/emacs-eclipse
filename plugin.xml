<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.commands">
      <category
            id="com.anirudh.flexclipse.emacs"
            name="Emacs">
      </category>
      <command
            categoryId="com.anirudh.flexclipse.emacs"
            id="com.anirudh.flexclipse.openinemacs"
            name="Open in Emacs">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="com.anirudh.emacseclipse.handlers.OpenEmacsHandler"
            commandId="com.anirudh.flexclipse.openinemacs">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="com.anirudh.flexclipse.commands.sampleCommand"
            contextId="org.eclipse.ui.contexts.window"
            sequence="M1+6"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
         <menu
               label="Emacs"
               mnemonic="E">
            <command
                  commandId="com.anirudh.flexclipse.openinemacs"
                  mnemonic="C"
                  style="push">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="com.anirudh.flexclipse.toolbars.sampleToolbar">
            <command
                  commandId="com.anirudh.flexclipse.openinemacs"
                  icon="icons/emacs_16.png"
                  tooltip="Open in Emacs"
                  id="com.anirudh.flexclipse.toolbars.openinemacs">
            </command>
         </toolbar>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="com.anirudh.emacseclipse.GnuClientPreferencePage"
            id="com.anirudh.flexclipse.gnuclientpage"
            name="Emacs">
      </page>
   </extension>

</plugin>
